//4.-Algoritmo para determinar el área de un rectángulo.
#include<stdio.h>  //biblioteca
int main(){  //funcion principal

float base,altura,area;//variables

//INICIO
printf("Ingrese la base\n");
scanf("%f",&base);
printf("Ingrese la altura\n");
scanf("%f",&altura);

area=base*altura;
printf("\nEl area del rectangulo es de %f",area);


return  0;	
}
